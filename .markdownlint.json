{
  "default": true,
  "no-trailing-punctuation": {
    "punctuation": ".,;:!"
  },
  "MD013": false,
  "MD024": false,
  "MD033": {
    "allowed_elements": [
      "cite",
      "code",
      "del",
      "dl",
      "dt",
      "dd",
      "script",
      "var"
    ]
  },
  "first-line-h1": false,
  "no-hard-tabs": true,
  "no-trailing-spaces": {
    "br_spaces": 2
  }
}
