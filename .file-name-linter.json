{
  "rules": {
    "jekyll/_posts": [
      "/[0-9\\-a-z]+\\.",
      "\\.md$"
    ],
    "jekyll/assets": [
      "/[0-9\\-a-z_]+\\.",
      "\\.(jpg|jpeg|png|gif|webp|zip|pdf|docx|doc|txt)$"
    ]
  },
  "ignore": [
    "jekyll/assets/js",
    "jekyll/assets/css",
    "jekyll/assets/images",
    "jekyll/assets/developers/wechaty",
    "tests/fixtures"
  ]
}
